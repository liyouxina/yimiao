page {
    background-color: #fff;
}

.mine-h-bg {
    width: 100%;
}

.mine-h-bg image {
    width: 100%;
    height: auto;
}

.mine-h-profile {
    width: 100%;
    top: 160rpx;
    left: 20rpx;
    z-index: 99;
}

.mine-h-pic {
    width: 158rpx;
    height: 158rpx;
    margin: 0 64rpx 0 32rpx;
    border-radius: 100%;
    overflow: hidden;
}

.mine-h-pic open-data {
    width: 100%;
}

.mine-h-profile {
    position: absolute;
    z-index: 999;
}

.mine-h-info {
    flex: 1;
    height: 200rpx;
    margin-top: 42rpx;
}

.mine-h-info-desc image {
    width: 40rpx;
    height: 40rpx;
}

.mine-h-info-desc,.mine-h-info-name {
    width: 100%;
    color: #fff;
    font-weight: 600;
}

.mine-h-info-name {
    font-size: 32rpx;
}

.mine-h-info-desc {
    margin-top: 14rpx;
    font-size: 24rpx;
}

.cell {
    width: 100%;
    margin-top: 52rpx;
}

.cell-item {
    width: 100%;
    height: 100rpx;
    margin-top: 48rpx;
    line-height: 100rpx;
    box-sizing: border-box;
    padding: 0 32rpx;
}

.cell-icon-pre {
    width: 100rpx;
    height: 100rpx;
}

.cell-icon-neg {
    width: 24rpx;
    height: 24rpx;
    margin-top: 28rpx;
}

.cell-label {
    flex: 1;
    font-size: 32rpx;
}

.success-icon {
    width: 140rpx;
    margin: 20vh auto 0;
}

.success-label {
    font-size: 36rpx;
    margin-top: 16rpx;
    color: #666;
    letter-spacing: .2em;
}

.success-btn {
    width: 448rpx;
    height: 80rpx;
    line-height: 80rpx;
    border-radius: 40rpx;
    margin-top: 90rpx;
    font-size: 36rpx;
    color: #fff;
    background-color: #5192ff;
    box-shadow: 4px 6px 26px 4px rgba(15,76,179,.21);
}

.empty {
    margin-top: 20vh;
}